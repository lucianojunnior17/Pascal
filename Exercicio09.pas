Program Exercicio09;
 var
 N1: Integer;
 N2: Integer;
 Begin
   Write('Digite o primeiro número: ');
   read(n1);
   Write('Digite o segundo número: ');
   read(n2);
  
    if (n1<n2)then
    begin
     Write('O número ' , n1,' é o menor!');
   end
   else
   begin
     Write('O número ' ,n2,' é o menor!');
   end;
End.
